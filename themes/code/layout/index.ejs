<h1><%- config.title %></h1>

<ul>
	<li><a href="https://twitter.com/rknightuk">twitter.com/rknightuk</a></li>
	<li><a href="https://github.com/rknightuk">github.com/rknightuk</a></li>
	<li><a href="https://rknight.me">rknight.me</a></li>
</ul>

<h2>Projects</h2>

<% if (site.data.projects) { %>
  <ul class="project-list">
    <% for(var obj in site.data.projects){ %>
        <li class="project-item">
          <%= site.data.projects[obj].name %>: <%- site.data.projects[obj].desc %>
          <ul>
          	<li><a href="/<%= site.data.projects[obj].path %>">Documentation</a></li>
          	<li><a href="<%= site.data.projects[obj].source %>">Source</a></li>
          </ul>
        </li>
    <% } %>
  </ul>
<% } %>